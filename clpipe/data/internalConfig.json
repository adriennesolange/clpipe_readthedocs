{
    "PostProcessingOptions2": {
        "WorkingDirectory": "",
        "WriteProcessGraph": true,
        "TargetImageSpace": "MNI152NLin2009cAsym",
        "ProcessingStepOptions": {
            "TemporalFiltering": {
                "Algorithm":"Butterworth",
                "FilteringHighPass": 0.008,
                "FilteringLowPass": -1,
                "FilteringOrder": 2
            }, 
            "IntensityNormalization": {
                "Algorithm": "10000_GlobalMedian"
            }, 
            "SpatialSmoothing": {
                "Algorithm": "SUSAN",
                "FWHM": 6
            },
            "AROMARegression":{
                "Algorithm": "fsl_regfilt"
            },
            "Resample":{
                "ReferenceImage": "SET REFERENCE IMAGE"
            },
            "ConfoundRegression": {
                "Algorithm": "afni_3dTproject",
                "Columns": ["csf", "csf_derivative1", "white_matter", "white_matter_derivative1"],
                "ConfoundFileName" : "confound_regressors.txt"
            }
        },
        "ConfoundOptions": {
            "Include": false,
            "Columns": ["csf", "csf_derivative1", "white_matter", "white_matter_derivative1"],
            "ProcessedConfoundsSuffix": "postprocessed"
        },
        "BatchOptions": {
            "MemoryUsage": "20000",
            "TimeUsage": "2:0:0",
            "NThreads": "1"
        }	
    }
}